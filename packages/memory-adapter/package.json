{
  "name": "@yaacl/memory-adapter",
  "version": "0.3.4",
  "description": "Memory adapter for yaacl",
  "main": "es5/index.js",
  "types": "es5/index.d.ts",
  "scripts": {
    "test": "jest",
    "coverage":
      "node node_modules/.bin/jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "prepublish": "tsc"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": ["acl", "security", "adapter"],
  "author": "jeanfortheweb",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeanfortheweb/yaacl"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "@types/node": "^9.3.0",
    "coveralls": "^3.0.0",
    "jest": "^22.1.4",
    "ts-jest": "^22.0.1",
    "typescript": "^2.6.2"
  },
  "jest": {
    "bail": true,
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": ["ts", "js"],
    "testRegex": "\\.spec\\.ts$",
    "mapCoverage": true
  },
  "dependencies": {
    "@yaacl/core": "^0.3.2"
  }
}
